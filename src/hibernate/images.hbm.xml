<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping package="com.models">
	<class name="Image" table="images">
		<id name="id" column="id">
			<generator class="native"/>
		</id>
		<property name="userId" type="java.lang.Integer" column="userId"/>
		<property name="imagePath" type="java.lang.String" column="imagePath"/>
		<property name="fileName" type="java.lang.String" column="fileName"/>
		<property name="uploaded" type="java.sql.Date" column="uploaded"/>
		<property name="shared" type="java.sql.Date" column="shared"/>
		<property name="lastEdited" type="java.sql.Date" column="lastEdited"/>
	</class>
</hibernate-mapping>